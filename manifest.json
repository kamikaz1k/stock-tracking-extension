{
  "manifest_version": 2,

  "name": "(Simple) Stock Tracker",
  "description": "This extension shows the stock prices for all the selected stocks",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Show Me The Money!"
  },
  "permissions": [
    "activeTab",
    "https://query.yahooapis.com/"
  ]
}